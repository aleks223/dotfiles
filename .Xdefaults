xterm*foreground: #D4D4D4
xterm*background: #181818
XTerm*cursorColor: #8FEA26
xterm*font: xft:Pragmata:size=8.5

! -- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hinstyle:   hintslight
Xft.lcdfilter: lcdlight
Xft.autohint: 1

! borderless and no scrollbar
URxvt*scrollBar_right: false
URxvt*scrollBar: false
URxvt*borderLess: false
URxvt.perl-ext-common: default,matcher,tabbed
URxvt.matcher.button:   1
URxvt.matcher.pattern.1:    \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt.matcher.launcher.1:   /usr/bin/jumanji
URxvt.matcher.pattern.2:    \\B(/\\S+?):(\\d+)(?=:|$)
URxvt.matcher.launcher.2:   gvim +$2 $1
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-bg: 1
URxvt.tabbed.tabbar-fg: 3

! -- Fonts -- !
URxvt.font:xft:Envy Code R:size=8.5

! Urxvt
URxvt.buffered: true
URxvt.background: #181818
URxvt.foreground: #D4D4D4
URxvt.cursorColor: #0081DE
URxvt.cursorBlink: true
URxvt.geometry: 100x30
URxvt.depth: 24

URxvt.title: Urxvt
URxvt.urlLauncher: chromium
URxvt.urgentOnBell: true
urxvt.visualBell: false
urxvt.mapAlert: true

